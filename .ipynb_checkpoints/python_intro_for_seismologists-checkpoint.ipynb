   "source": [
    "#!/usr/bin/env python\n",
    "import glob\n",
    "from obspy.core import read"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [],
   "source": [
    "for file in glob.glob('*.z'):\n",
    "    st = read(file)\n",
    "    tr = st[0]\n",
    "    msg = \"%s %s %f %f\" % (tr.stats.station, str(tr.stats.starttime),\n",
    "                           tr.data.mean(), tr.data.std())\n",
    "    print(msg)"
   ]
  }
 ],
